    </item>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#636468" />
            <corners android:bottomRightRadius="@dimen/dp_25" android:topRightRadius="@dimen/dp_25" />
        </shape>
    </item>
</selector>